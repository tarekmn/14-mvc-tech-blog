

{{#each blogs as |blog| }}

<div class="card">
  <div class="card-header">
    <h4>{{ blog.title }}</h4>
  </div>
  <div class="card-body">
    <p class="card-title"> {{ format_time blog.createdAt }}</p>
    <p class="card-text">{{ blog.description }}</p>
    {{!-- <a href="#" class="btn btn-primary">Button 1</a>
    <a href="#" class="btn btn-primary">Button 2</a> --}}
  </div>
</div>

{{/each}}
