

{{#each blogs as |blog| }}


<div class="card">
  <div class="card-header">
    <h4>{{ blog.title }}</h4>
  </div>
  <div class="card-body">
    <p class="card-title"> {{ format_time blog.createdAt }}</p>
    <p class="card-text">{{ blog.description }}</p>
    <a href="#" class="btn btn-primary">Info</a>
    <a href="#" class="btn btn-primary">Comment</a>
  </div>
</div>

{{/each}}
